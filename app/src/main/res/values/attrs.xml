<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ShapeIconView">
        <attr name="siv_color" format="color|reference"/>
        <attr name="siv_shadow_color" format="color|reference"/>
        <attr name="siv_shadow_radius" format="dimension|reference"/>
        <attr name="siv_shadow_size" format="dimension|reference"/>
        <attr name="siv_icon" format="reference"/>
        <attr name="siv_icon_size" format="enum">
            <enum name="smallest" value="0"/>
            <enum name="small" value="1"/>
            <enum name="normal" value="2"/>
            <enum name="big" value="3"/>
        </attr>
        <attr name="siv_shape" format="enum">
            <enum name="circle" value="0"/>
            <enum name="rect" value="1"/>
            <enum name="super_ellipse" value="2"/>
            <enum name="tear_drop" value="3"/>
            <enum name="shield" value="4"/>
            <enum name="lemon" value="5"/>
            <enum name="round_rect" value="6"/>
        </attr>
    </declare-styleable>
</resources>